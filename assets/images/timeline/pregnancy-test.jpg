<svg xmlns="http://www.w3.org/2000/svg" width="400" height="300" viewBox="0 0 400 300">
  <defs>
    <linearGradient id="bg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#ffeef7;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#f8d7da;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- 背景 -->
  <rect width="400" height="300" fill="url(#bg)"/>
  
  <!-- 验孕棒主体 -->
  <rect x="150" y="100" width="100" height="20" rx="10" fill="#ffffff" stroke="#e0e0e0" stroke-width="2"/>
  
  <!-- 验孕棒手柄 -->
  <rect x="120" y="105" width="40" height="10" rx="5" fill="#f0f0f0" stroke="#d0d0d0" stroke-width="1"/>
  
  <!-- 显示窗口 -->
  <rect x="170" y="105" width="15" height="10" rx="2" fill="#ffffff" stroke="#ccc" stroke-width="1"/>
  
  <!-- 两条线（阳性结果） -->
  <line x1="175" y1="107" x2="175" y2="113" stroke="#ff69b4" stroke-width="2"/>
  <line x1="180" y1="107" x2="180" y2="113" stroke="#ff69b4" stroke-width="2"/>
  
  <!-- 装饰性心形 -->
  <path d="M200 140 C200 135, 210 135, 210 145 C210 135, 220 135, 220 140 C220 150, 210 160, 210 160 C210 160, 200 150, 200 140 Z" fill="#ff69b4" opacity="0.6"/>
  
  <!-- 文字 -->
  <text x="200" y="200" text-anchor="middle" font-family="Arial, sans-serif" font-size="16" fill="#666">验孕测试</text>
  <text x="200" y="220" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" fill="#999">Pregnancy Test</text>
</svg>