<svg xmlns="http://www.w3.org/2000/svg" width="400" height="300" viewBox="0 0 400 300">
  <defs>
    <linearGradient id="ultrasoundBg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#1a1a1a;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#2d2d2d;stop-opacity:1" />
    </linearGradient>
    <radialGradient id="babyGlow" cx="50%" cy="50%" r="50%">
      <stop offset="0%" style="stop-color:#ffffff;stop-opacity:0.8" />
      <stop offset="100%" style="stop-color:#cccccc;stop-opacity:0.3" />
    </radialGradient>
  </defs>
  
  <!-- 超声波背景 -->
  <rect width="400" height="300" fill="url(#ultrasoundBg)"/>
  
  <!-- 扫描线效果 -->
  <line x1="0" y1="50" x2="400" y2="50" stroke="#444" stroke-width="1" opacity="0.5"/>
  <line x1="0" y1="100" x2="400" y2="100" stroke="#444" stroke-width="1" opacity="0.5"/>
  <line x1="0" y1="150" x2="400" y2="150" stroke="#444" stroke-width="1" opacity="0.5"/>
  <line x1="0" y1="200" x2="400" y2="200" stroke="#444" stroke-width="1" opacity="0.5"/>
  <line x1="0" y1="250" x2="400" y2="250" stroke="#444" stroke-width="1" opacity="0.5"/>
  
  <!-- 胎儿轮廓 -->
  <ellipse cx="200" cy="150" rx="60" ry="40" fill="url(#babyGlow)" opacity="0.7"/>
  
  <!-- 头部 -->
  <circle cx="180" cy="130" r="25" fill="#ffffff" opacity="0.6"/>
  
  <!-- 身体 -->
  <ellipse cx="200" cy="160" rx="35" ry="25" fill="#ffffff" opacity="0.5"/>
  
  <!-- 心跳点 -->
  <circle cx="190" cy="155" r="3" fill="#ff6b6b" opacity="0.8">
    <animate attributeName="opacity" values="0.8;0.3;0.8" dur="1s" repeatCount="indefinite"/>
  </circle>
  
  <!-- 测量线 -->
  <line x1="150" y1="120" x2="210" y2="120" stroke="#00ff00" stroke-width="1" opacity="0.8"/>
  <text x="180" y="115" text-anchor="middle" font-family="monospace" font-size="10" fill="#00ff00">2.3cm</text>
  
  <!-- 超声波信息 -->
  <text x="20" y="30" font-family="monospace" font-size="12" fill="#00ff00">ULTRASOUND</text>
  <text x="20" y="280" font-family="monospace" font-size="10" fill="#00ff00">8W+3D</text>
  <text x="320" y="280" font-family="monospace" font-size="10" fill="#00ff00">HR: 165 BPM</text>
  
  <!-- 装饰性边框 -->
  <rect x="10" y="10" width="380" height="280" fill="none" stroke="#333" stroke-width="2"/>
</svg>