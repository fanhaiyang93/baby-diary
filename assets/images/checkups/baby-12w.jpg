<svg xmlns="http://www.w3.org/2000/svg" width="400" height="300" viewBox="0 0 400 300">
  <defs>
    <linearGradient id="wombBg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#2a2a2a;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#1a1a1a;stop-opacity:1" />
    </linearGradient>
    <radialGradient id="babyHighlight" cx="50%" cy="50%" r="60%">
      <stop offset="0%" style="stop-color:#ffffff;stop-opacity:0.9" />
      <stop offset="100%" style="stop-color:#dddddd;stop-opacity:0.4" />
    </radialGradient>
  </defs>
  
  <!-- 超声波背景 -->
  <rect width="400" height="300" fill="url(#wombBg)"/>
  
  <!-- 网格线 -->
  <defs>
    <pattern id="grid" width="20" height="20" patternUnits="userSpaceOnUse">
      <path d="M 20 0 L 0 0 0 20" fill="none" stroke="#333" stroke-width="0.5" opacity="0.3"/>
    </pattern>
  </defs>
  <rect width="400" height="300" fill="url(#grid)"/>
  
  <!-- 12周胎儿轮廓 -->
  <ellipse cx="200" cy="150" rx="80" ry="60" fill="url(#babyHighlight)" opacity="0.6"/>
  
  <!-- 头部（更大更清晰） -->
  <circle cx="170" cy="120" r="35" fill="#ffffff" opacity="0.8"/>
  
  <!-- 脊柱 -->
  <path d="M 180 140 Q 190 160 200 180 Q 210 200 220 220" stroke="#ffffff" stroke-width="3" fill="none" opacity="0.6"/>
  
  <!-- 手臂 -->
  <ellipse cx="150" cy="160" rx="15" ry="8" fill="#ffffff" opacity="0.5" transform="rotate(-30 150 160)"/>
  <ellipse cx="250" cy="160" rx="15" ry="8" fill="#ffffff" opacity="0.5" transform="rotate(30 250 160)"/>
  
  <!-- 腿部 -->
  <ellipse cx="180" cy="200" rx="12" ry="20" fill="#ffffff" opacity="0.5" transform="rotate(-15 180 200)"/>
  <ellipse cx="220" cy="200" rx="12" ry="20" fill="#ffffff" opacity="0.5" transform="rotate(15 220 200)"/>
  
  <!-- 心脏位置 -->
  <circle cx="185" cy="155" r="4" fill="#ff6b6b" opacity="0.9">
    <animate attributeName="r" values="4;6;4" dur="0.8s" repeatCount="indefinite"/>
  </circle>
  
  <!-- 测量标记 -->
  <line x1="130" y1="100" x2="240" y2="100" stroke="#00ff00" stroke-width="1.5" opacity="0.9"/>
  <text x="185" y="95" text-anchor="middle" font-family="monospace" font-size="12" fill="#00ff00">CRL: 5.4cm</text>
  
  <!-- 超声波参数 -->
  <text x="20" y="25" font-family="monospace" font-size="14" fill="#00ff00">12 WEEKS SCAN</text>
  <text x="20" y="45" font-family="monospace" font-size="10" fill="#00ff00">BPD: 2.1cm</text>
  <text x="20" y="60" font-family="monospace" font-size="10" fill="#00ff00">FL: 0.9cm</text>
  
  <text x="20" y="280" font-family="monospace" font-size="10" fill="#00ff00">12W+2D</text>
  <text x="300" y="280" font-family="monospace" font-size="10" fill="#00ff00">HR: 158 BPM</text>
  
  <!-- 边框 -->
  <rect x="5" y="5" width="390" height="290" fill="none" stroke="#444" stroke-width="2"/>
</svg>